{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Verbose",
      "Override": {
        "Microsoft": "Verbose",
        "System": "Verbose",
        "Microsoft.AspNetCore.Authentication": "Verbose",
        "Microsoft.EntityFrameworkCore.Database.Command": "Verbose"
      }
    },
    "WriteTo": [
      {
        "Name": "RollingFile",
        "Args": {
          "restrictedToMinimumLevel": "Verbose",
          "pathFormat": "Logs\\log-{Date}.json",
          "textFormatter": "Serilog.Formatting.Json.JsonFormatter, Serilog",
          "fileSizeLimitBytes": 2147483648,
          "retainedFileCountLimit": 2,
          "rollingInterval": "Day"
        }
      },
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "===> {Timestamp:HH:mm:ss.fff zzz} [{Level}] {Message}{NewLine}{Exception}"
        }
      }
    ]
  },
  "AllowedHosts": "*",
  "AllowedOrigins": "",
  "JwtIssuerOptions": {
    "Issuer": "",
    "Audience": ""
  },
  "ClientOptions": {
    "Address": ""
  },
  "EmailServiceOptions": {
    "SystemAdminName": "bot-leadslyDotNetApiStarterAdmin",
    "SystemAdminEmail": "SNFPOW@gmail.com",
    "SmtpServer": "smtp.gmail.com",
    "Port": "465",
    "PasswordReset": {
      "EmailSubject": "Password Reset"
    },
    "VerifyEmail": {
      "EmailSubject": "Verify Email"
    },
    "ChangeEmail": {
      "EmailSubject": "Email Change Request"
    }
  },
  "TwoFactorAuthDisplayAppName": "bot-leadslyDotNetApiStarter"
}
